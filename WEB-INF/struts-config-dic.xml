			<form-property name="cardno" type="java.lang.String"/>
			<form-property name="province" type="java.lang.String"/>
			<form-property name="city" type="java.lang.String"/>
			<form-property name="county" type="java.lang.String"/>
			<form-property name="address" type="java.lang.String"/>
			<form-property name="year" type="java.lang.String"/>
			<form-property name="month" type="java.lang.String"/>
			<form-property name="day" type="java.lang.String"/>
			<form-property name="hour" type="java.lang.String"/>
			<form-property name="content" type="java.lang.String" />
			<form-property name="name" type="java.lang.String" />                   
			<form-property name="gender" type="java.lang.String" />
			<form-property name="cookies" type="java.lang.String"/>
			<form-property name="type" type="java.lang.String" />
			<form-property name="date" type="java.lang.String" />
