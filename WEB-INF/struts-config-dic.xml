			<form-property name="getdate" type="java.lang.String"/>
			<form-property name="enddate" type="java.lang.String"/>
			<form-property name="consumdate" type="java.lang.String"/>
			<form-property name="location" type="java.lang.String"/>
			<form-property name="checkcode" type="java.lang.String"/>
			<form-property name="page" type="java.lang.String"/>
