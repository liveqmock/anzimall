			<form-property name="defaultstate" type="java.lang.String"/>
		</form-bean>		
	</form-beans>

	<action-mappings>		
		<action path="/dic"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="DiCForm" parameter="method" scope="request"
			validate="false">
			<forward name="to_user_address" path="/cglib/user/address.jsp" />
			<forward name="to_user_cbd" path="/cglib/user/cbd.jsp" />
			<forward name="to_user_detail" path="/cglib/user/userdetail.jsp" />
			<forward name="to_user_vipcard" path="/cglib/user/vipcard.jsp" />
			<forward name="to_register_over" path="/cglib/register/register_over.jsp" />
			<forward name="to_login" path="/cglib/login.jsp" />
			<forward name="to_user_bankcard" path="/cglib/user/bankcard.jsp" />
			<forward name="user_password" path="/cglib/user/password.jsp" />
			<forward name="to_index" path="/cglib/index.jsp" />
			<forward name="to_user_coupons" path="/cglib/user/coupons.jsp" />
			
			<forward name="to_register" path="/cglib/register/register_info.jsp" />
			
			<forward name="to_change_password" path="/cglib/password/password_reset.jsp" />
			
			<forward name="to_user_spread" path="/cglib/user/spread.jsp" />
			
			<forward name="help" path="/cglib/article/help.jsp" />
				
		</action>		
	</action-mappings>

