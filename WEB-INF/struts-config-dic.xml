			<form-property name="phone" type="java.lang.String"/>
			<form-property name="nick" type="java.lang.String"/>
			<form-property name="code" type="java.lang.String"/>
			<form-property name="postcode" type="java.lang.String"/>
			<form-property name="p" type="java.lang.String"/>
			<form-property name="birthday" type="java.lang.String"/>
			<form-property name="vipcardtype" type="java.lang.String"/>
			<form-property name="cardvaliditybegin" type="java.lang.String"/>
			<form-property name="cardvalidityend" type="java.lang.String"/>
			<form-property name="storename" type="java.lang.String"/>
			<form-property name="vipcardno" type="java.lang.String"/>
			<form-property name="carddate" type="java.lang.String"/>
			<form-property name="consumetotal" type="java.lang.String"/>
